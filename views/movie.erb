<div class="col-md-4">
  <img src="<%= @movie.poster %>">
</div>
<dl class="dl-horizontal col-md-4">
  <dt>Title</dt>
  <dd><%= @movie.title%></dd>
  <dt>Year</dt>
  <dd><%= @movie.year%></dd>
  <dt>Genre</dt>
  <dd><%= @movie.genre %></dd>
  <dt>Director</dt>
  <dd><%= @movie.director %></dd>
  <dt>Writer</dt>
  <dd><%= @movie.writer %></dd>
  <dt>Actors</dt>
  <dd><%= @movie.actors %></dd>
  <dt>Description</dt>
  <dd><%= @movie.description %></dd>
</dl>
